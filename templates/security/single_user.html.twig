{% extends 'base.html.twig' %}

{% block title %}

    {% if user is not null %}
        Panel użytkownika: {{ user.getUserFullName }}
    {% else %}
        Nowy użytkownik
    {% endif %}

{% endblock %}

{% block body %}
    <user-single
        {% if user is not null %}
            :user-id="{{ user.getId }}"
        {% endif %}
    ></user-single>
{% endblock %}

